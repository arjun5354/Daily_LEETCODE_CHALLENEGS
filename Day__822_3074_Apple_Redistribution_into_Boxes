class Solution {
public:
    int minimumBoxes(vector<int>& apple, vector<int>& capacity) {
        int a_sum=accumulate(apple.begin(),apple.end(),0);
        sort(capacity.begin(),capacity.end());
        int cnt=0;
        for(int i=capacity.size()-1;i>=0;i--){
            a_sum-=capacity[i];
            cnt++;
            if(a_sum<=0) return cnt;
        }
        return cnt;
    }
};
