class Solution {
public:
    int longestBalanced(vector<int>& nums) {
        int n=nums.size(),result=0;
        for(int i=0;i<n;i++){
            unordered_map<int,int>even;
            unordered_map<int,int>odd;
            for(int j=i;j<n;j++){
                if(nums[j]&1)odd[nums[j]]++;
                else even[nums[j]]++;
                if(even.size()==odd.size())result=max(result,j-i+1);
            }
        }
        return result;
    }
};
