class Solution {
public:
    int countTriples(int n) {
        unordered_set<int>st;
        for(int i=1;i<=n;i++)st.insert(i*i);
        int result=0;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                if(st.count(i*i+j*j))result++;
            }
        }
        return result;
    }
};
